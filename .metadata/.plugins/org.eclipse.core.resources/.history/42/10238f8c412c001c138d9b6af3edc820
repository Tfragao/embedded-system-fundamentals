#include <stdio.h>
#include <stdlib.h>
#include<stdint.h>

struct carModel
{
	uint32_t carNumber;
	uint32_t carPrice;
	uint16_t carMaxSpeed;
	float carWeight;
};

int main(void)
{

	struct carModel BMW = {2021, 1500, 220, 1330};
	struct carModel Ford = {4031, 35000,160,1900.96};

	return EXIT_SUCCESS;
}
